<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent"
    <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content"
        <LinearLayout android:layout_width="wrap_content" android:layout_height="wrap_content">
            <ImageView android:id="@+id/icon" android:layout_width="UNKNOWN_DATA_0x6401" android:layout_height="UNKNOWN_DATA_0x6401" android:contentDescription="@string/image" />
        </LinearLayout>
        <LinearLayout android:orientation="vertical" android:layout_width="wrap_content" android:layout_height="wrap_content">
            <TextView android:id="@+id/tvSelectedItemName" android:paddingLeft="10dp" android:paddingBottom="10dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxWidth="300dp" android:text="hello" android:singleLine="false" android:layout_weight="1.0" style="@style/RestText" />
            <TextView android:id="@+id/tvSelectedItemCat" android:paddingLeft="10dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="hello" style="@style/RestSmallerText" />
            <LinearLayout android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <TextView android:id="@+id/tvCurrency" android:paddingLeft="10dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="" style="@style/RestSmallerText" />
                <TextView android:id="@+id/tvSelectedItemPrice" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="hello" style="@style/RestSmallerText" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="left|right|center_horizontal|fill_horizontal|center|fill|start|end" android:layout_gravity="left|right|center_horizontal|fill_horizontal|center|fill|start|end" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
            <TextView android:id="@+id/tvQty" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Qty" style="@style/RestSmallerText" />
            <LinearLayout android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <Button android:textSize="12sp" android:textColor="#ffffff" android:id="@+id/btnPlus" android:background="@drawable/button" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="+" />
                <EditText android:enabled="false" android:textSize="16sp" android:id="@+id/etQty" android:background="0x1080018" android:layout_width="wrap_content" android:layout_height="match_parent" android:text="1" android:inputType="number|numberSigned|numberDecimal|numberPassword|phone" />
                <Button android:textSize="12sp" android:textColor="#ffffff" android:id="@+id/btnMinus" android:background="@drawable/button" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="-" />
            </LinearLayout>
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="NET TOTAL" style="@style/RestSmallerText" />
            <TextView android:id="@+id/tvTotalPrice" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="NET TOTAL" style="@style/RestSmallerText" />
        </LinearLayout>
    </LinearLayout>
    <TextView android:id="@+id/tvSelectedItemDesc" android:paddingLeft="110dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="hello" style="@style/RestSmallerText" />
    <TextView android:id="@+id/tvSelectedId" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" />
    <TextView android:id="@+id/tvSelectedDate" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" />
</LinearLayout>
